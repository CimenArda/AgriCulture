@using EntityLayer.Concrete
@model List<Service>


<div class="sevices-w3layouts" id="services">
	<div class="container">
		<h3 class="tittle">Hizmetlerimiz</h3>
		<p class="heading-btm-w3ls">
			Sürdürülebilir tarım çözümleri, toprak yönetimi, su kaynaklarının etkin kullanımı ve çevresel dengeyi destekleyerek, müşterilerimize çevresel açıdan sorumlu ve verimli tarım uygulamaları sunmaktayız.
		</p>
		<div class="services-main">
			@foreach (var item in Model)
			{
				
		
			<div class="@item.Class1" data-wow-delay=".5s">
				<h5>@item.Title</h5>
				<p>@item.Description</p>

				<div class="@item.Class2">
					<i class="@item.Image" aria-hidden="true"></i>
					<p>Sizler İçin..</p>
				</div>
			</div>


			}

		</div>
		<div class="clearfix"> </div>
	</div>
</div>