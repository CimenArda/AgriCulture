@model EntityLayer.Concrete.Contact

<div class="col-md-9 contact-middle">
	<h2>Selam Ver!</h2>
	<form method="post">
		<div class="form-agileinfo">
			<p>Name</p>
			<input type="text" name="your name" placeholder="Ad-Soyad" required="" />
			<p>Email</p>
			<input type="email" name="your email" placeholder="Mail Adresi" required="" />
		</div>
		<div class="form-agileits-w3layouts">
			<p>Comments</p>
			<textarea name="your message" placeholder="Mesajınız.." required=""></textarea>
			<input type="submit" value="Mesajı Gönder">
		</div>
		<div class="clearfix"></div>
	</form>
</div>

